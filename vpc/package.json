{
  "name": "vpc",
  "version": "2.1.0",
  "bin": "bin/vpc.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "clean": "find . -name '*.js' -not -path './node_modules/*' -not -path './cdk.out/*' -not -name '*.config.js' -delete; find . -name '*.d.ts' -not -path './node_modules/*' -not -path './cdk.out/*' -delete; rm -rf cdk.out node_modules/.cache; find . -name '*.tsbuildinfo' -delete 2>/dev/null || true",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "aws-cdk": "2.1100.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.233.0",
    "cdk-nag": "^2.37.55",
    "constructs": "^10.4.4",
    "shared": "*",
    "source-map-support": "^0.5.21"
  }
}
