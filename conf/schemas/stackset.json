{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "patternProperties": {
        "^[a-z]{2}-[a-z]+-[0-9]+$": {
            "type": "object",
            "properties": {
                "permission_model": {
                    "type": "string",
                    "enum": [
                        "SERVICE_MANAGED"
                    ]
                },
                "auto_deployment": {
                    "type": "boolean"
                },
                "account_filter_type": {
                    "type": "string",
                    "enum": [
                        "NONE"
                    ]
                },
                "organizational_unit_ids": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "pattern": "^ou-[a-zA-Z0-9]+$"
                    },
                    "minItems": 1
                },
                "failure_tolerance_percentage": {
                    "type": "integer",
                    "minimum": 0
                },
                "max_concurrent_percentage": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 100
                },
                "region_concurrency_type": {
                    "type": "string",
                    "enum": [
                        "PARALLEL"
                    ]
                },
                "call_as": {
                    "type": "string",
                    "enum": [
                        "DELEGATED_ADMIN"
                    ]
                },
                "stack_regions": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "pattern": "^[a-z]{2}-[a-z]+-[0-9]+$"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "permission_model",
                "auto_deployment",
                "account_filter_type",
                "organizational_unit_ids",
                "failure_tolerance_percentage",
                "max_concurrent_percentage",
                "region_concurrency_type",
                "call_as",
                "stack_regions"
            ]
        }
    },
    "additionalProperties": false
}